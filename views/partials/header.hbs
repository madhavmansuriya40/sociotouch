<html>
<body>
<script type="text/javascript" src="/home_assets/js/uaest.js"></script>

<link rel="stylesheet" type="text/css" href="/home_assets/css/animate.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/bootstrap.min.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/line-awesome.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/line-awesome-font-awesome.min.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/font-awesome.min.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/jquery.mCustomScrollbar.min.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/slick.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/slick-theme.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/style.css">
<link rel="stylesheet" type="text/css" href="/home_assets/css/responsive.css">
<link rel="stylesheet" type="text/css" href="/stylesheets/sweet-alert.css">

<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.0/css/all.css">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
<link rel='stylesheet prefetch' href='https://cdn.rawgit.com/t4t5/sweetalert/v0.2.0/lib/sweet-alert.css'>
<script src="/home_assets/css/jquery.mousewheel.min.js"></script>


<style>
    .text_style {
        white-space: nowrap;
        width: 100px;
        overflow: hidden;
        text-overflow: ellipsis;
    }
</style>
<input type="hidden" id="_csrf" name="_csrf" value="{{ csrfToken }}">
<header>
    <div class="container">
        <div class="header-data">
            <div class="logo">
                <a href="#" title=""><img src="/home_assets/img/logo.png" alt="" width="50"></a>
            </div><!--logo end-->
            <div class="search-bar">
                <link rel="stylesheet" type="text/css" href="/stylesheets/temp.css"/>
                <div class="input-group input-group-sm" style="margin-top: 18px;width: 80%">
                    <input type="text" id="user_search" placeholder="Search..." class="form-control"
                           style="font-size: 18px">
                    <span class="input-group-btn">
                      <button type="button" id="btn_search_user" class="btn" style="background-color: #88098d;"><em
                              class="fa fa-search color-white"></em></button>
                    </span>
                </div>
                <style>
                    ::-webkit-scrollbar {
                        width: 0px; /* remove scrollbar space */
                        background: transparent; /* optional: just make scrollbar invisible */
                    }

                    /* optional: show position indicator in red */
                    ::-webkit-scrollbar-thumb {
                        background: transparent;
                    }
                </style>
                <div id="div_srch_data"
                     style="position:absolute;background: white;width: inherit;z-index: 9999;border: solid #0e2549 2px;border-radius: 25px;border-top: none;max-height: 80%;overflow: scroll;">
                    <ul id="drp_dwn_result">

                    </ul>
                </div>
                <!--<form>-->
                <!--<input type="text" name="search" placeholder="Search...">-->
                <!--<button ><i class="fa fa-search" style="color: #88098d;"></i></button>-->
                <!--</form>-->
            </div><!--search-bar end-->
            <nav>
                <ul>
                    <li>
                        <a href="/users/home" title="">
                            <span class="ico" style=""><i class="fa fa-home"></i> </span>
                            <span style="font-family: 'Bell MT';">Home</span>
                        </a>
                    </li>
                    <li>
                        <a href="/users/request" title="">
                            <span class="ico"><i class="fa fa-users"></i> </span>
                            <span style="font-family: 'Bell MT';">Requests</span>
                        </a>
                    </li>
                    <li>
                        <a href="/users/myprofile" title="">
                            <span class="ico"><i class="fas fa-user-tie"></i> </span>
                            <span style="font-family: 'Bell MT';">Profile</span>
                        </a>
                    </li>
                    <li>
                        <a href="/users/chatroom" title="" class="">
                            <span class="ico"><i class="fas fa-comment"></i></span>
                            <span style="font-family: 'Bell MT';">Chat Room</span>
                        </a>
                    </li>
                    <li>
                        <a href="/users/group" title="" class="">
                            <span class="ico"><i class="fas fa-plus"></i></span>
                            <span style="font-family: 'Bell MT';">new</span>
                        </a>
                    </li>

                </ul>
            </nav><!--nav end-->
            <div class="menu-btn">
                <a href="#" title=""><i class="fa fa-bars"></i></a>
            </div><!--menu-btn end-->
            <div class="user-account">
                <div class="user-info">
                    {{#each user_info}}
                    <img src="{{this.user_img_path}}" alt="" width="50" height="50"
                         style="border: white solid 2px;box-shadow: 2px 2px 5px #1b1e21">
                    {{/each}}
                    <a href="#" title="" style="margin-top: 18px">{{#each user_info}}
                        <div class="text_style color-blue" style="font-size: 20px">{{this.user_uname}}</div>
                        {{/each}}
                    </a>
                    <i class="fa fa-sort-down"></i>
                </div>
                <div class="user-account-settingss">
                    <br>
                    <center><span class="text-center color-orange" style="font-weight: bolder">{{#each user_info}}
                       <a href="/users/myprofile"> {{this.user_uname}}</a>
                        {{/each}}</span></center>
                    <h3><span class="color-purple" style="font-weight: bolder">Online Status</span></h3>
                    <ul class="on-off-status">
                        <li class="text-center">
                            <div class="fgt-sec">
                                <input type="checkbox" name="cc" id="c5">
                                <label for="c5">
                                    <span></span>
                                </label>
                                <small>Online</small>
                            </div>
                        </li>
                    </ul>
                    <h3><span class="color-purple" style="font-weight: bolder">Setting</span></h3>
                    <ul class="us-links">
                        <li><a href="#" title=""><i class="fa fa-gear fa-spin color-blue" style="font-size: 20px"></i>
                            Account Setting</a></li>
                        <li><a href="#" title=""><i class="fa fa-lock color-green" style="font-size: 20px"></i> Privacy</a>
                        </li>
                        <li><a href="/users/saved" title=""><i class="fa fa-bookmark color-purple" style="font-size: 20px"></i>
                            Saved</a>
                        </li>
                        <li><a href="/users/manage_profile" title=""><i class="fa fa-user color-orange" style="font-size: 20px"></i>
                            My Profile</a>
                        </li>
                        <li><a href="#" title=""><i class="fa fa-file-archive-o color-violet"
                                                    style="font-size: 20px"></i> Terms &amp; Conditions</a></li>
                    </ul>
                    <h3 class="tc"><a href="/logout" title=""><i class="fa fa-power-off color-red"> Logout</i></a></h3>
                </div><!--user-account-settingss end-->
            </div>
        </div><!--header-data end-->
    </div>
</header><!--header end-->


<script src="/javascripts/user_home.js"></script>
<script src='https://cdn.rawgit.com/t4t5/sweetalert/v0.2.0/lib/sweet-alert.min.js'></script>
<script type="text/javascript" src="/javascripts/emoji.js"></script>
<script type="text/javascript" src="/javascripts/sweet-alert.min.js"></script>
<script type="text/javascript" src="/javascripts/manage_user_request.js"></script>
<script type="text/javascript" src="/javascripts/quote.js"></script>
<script type="text/javascript" src="/home_assets/js/script.js"></script>
<script type="text/javascript" src="/home_assets/js/jquery.min.js"></script>
<script type="text/javascript" src="/home_assets/js/popper.js"></script>
<script type="text/javascript" src="/home_assets/js/bootstrap.min.js"></script>
<script type="text/javascript" src="/home_assets/js/jquery.mCustomScrollbar.js"></script>
<script type="text/javascript" src="/home_assets/js/slick.min.js"></script>
<script type="text/javascript" src="/home_assets/js/scrollbar.js"></script>
</body>
</html>