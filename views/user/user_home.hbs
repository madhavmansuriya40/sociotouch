<script>
    $(document).ready(function () {
        var x = document.getElementById("loc_ans");
        var lat = "";
        var long = "";
        var loc = "";

        function showPosition(position) {
            lat = position.coords.latitude;
            long = position.coords.longitude;
            x.innerHTML = "Latitude: " + position.coords.latitude +
                "<br>Longitude: " + position.coords.longitude;

            loc = lat + "," + long;
        }

        var html_data = "";
        $("#txt_location_search_bar, #txt_multi_location_search_bar").keyup(function () {

            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(showPosition);
            } else {
                x.innerHTML = "Geolocation is not supported by this browser.";
            }

            var location = "";

            if ($("#txt_location_search_bar").val().trim() != "" || $("#txt_multi_location_search_bar").val().trim() != "") {

                $("#show_search_rslt").html("");
                $("#multi_show_search_rslt").html("");

                if (!($("#txt_location_search_bar").val().trim() == "")) {
                    location = document.getElementById("txt_location_search_bar").value;
                }
                else {
                    location = document.getElementById("txt_multi_location_search_bar").value;
                }
                $.get("https://places.api.here.com/places/v1/autosuggest", {
                    at: loc,
                    app_id: 'BjmHZnahwYeQWLjbemD7',
                    app_code: 'hfv2htIlBXllnGcyOo7CBw',
                    q: location
                }, function (data) {
                    html_data = "";
                    if (data != "") {
                        if ($("#txt_location_search_bar").val().trim() != "") {
                            for (var i = 0; i < data.results.length; i++) {
                                html_data = html_data + '<p style="margin-left: 2%;margin-top: 0px;margin-bottom:-15px;cursor: pointer" class="location_area" id=' + data.results[i].id + ' title="' + data.results[i].title + '-' + data.results[i].highlightedVicinity + '" >' + data.results[i].highlightedTitle + ' <b style="color: red">-</b> ' + data.results[i].highlightedVicinity + '</p><hr>';
                            }
                        }
                        else {
                            for (var i = 0; i < data.results.length; i++) {
                                html_data = html_data + '<p style="margin-left: 2%;margin-top: 0px;margin-bottom:-15px;cursor: pointer" class="multi_location_area" id=' + data.results[i].id + ' title="' + data.results[i].title + '-' + data.results[i].highlightedVicinity + '" >' + data.results[i].highlightedTitle + ' <b style="color: red">-</b> ' + data.results[i].highlightedVicinity + '</p><hr>';
                            }
                        }
                    }
                    else {
                        html_data = html_data + "No result found";
                    }
                });
                if ($("#txt_location_search_bar").val().trim() != "") {
                    $("#show_search_rslt").html(html_data);
                }
                else {
                    $("#multi_show_search_rslt").html(html_data);
                }
                if ($("#txt_location_search_bar").val().trim() != "") {
                    $("#div_show_search_loc_res ").show();
                    $("#txt_location_search_bar").slideDown(700);
                }
                else {
                    $("#div_multi_show_search_loc_res").show();
                    $("#txt_multi_location_search_bar").slideDown(700);
                }
            }
            else {
                $("#div_show_search_loc_res").hide(500);
                $("#div_multi_show_search_loc_res").hide(500);
                $("#show_search_rslt").html("");
                $("#multi_show_search_rslt").html("");
            }
        });
    });

</script>

